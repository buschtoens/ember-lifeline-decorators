{
  "name": "ember-lifeline-decorators",
  "version": "2.0.0",
  "description": "Decorators for easily using ember-lifeline",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/buschtoens/ember-lifeline-decorators",
  "license": "MIT",
  "author": "Jan Buscht√∂ns <jan@buschtoens.me> (https://jan.buschtoens.me/)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --ext js,ts .",
    "lint:types": "tsc",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "@ember-decorators/utils": "^6.1.1",
    "ember-auto-import": "^1.6.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-typescript": "^3.1.4",
    "ember-destroyable-polyfill": "^2.0.1",
    "patch-method": "^0.3.1"
  },
  "devDependencies": {
    "@clark/eslint-config-ember-typescript": "^1.26.5",
    "@clark/eslint-config-node": "^1.26.5",
    "@ember/optional-features": "^1.3.0",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^3.4.9",
    "@types/ember-resolver": "^5.0.9",
    "@types/ember-test-helpers": "^1.0.8",
    "@types/ember-testing-helpers": "^0.0.4",
    "@types/ember__test-helpers": "^1.7.2",
    "@types/qunit": "^2.9.1",
    "@types/rsvp": "^4.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "delay": "^4.4.0",
    "ember-cli": "~3.20.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-lifeline": "^6.0.0",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.20.3",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.6.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.4.0",
    "release-it": "^13.6.5",
    "release-it-lerna-changelog": "^2.3.0",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "ember-lifeline": "^5.0.0"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
